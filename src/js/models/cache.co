class Poems.Models.Cache
  constructor: ->
    @data = new Map

  set: (key, item) ->
    @data.set key, item
    @cleanup key if @data.size > 20

  get: (key) ->
    @data.get key

  has: (key) ->
    @data.has key

  cleanup: (key) ->
    @removeAllKeysBut [key]

  removeAllKeysBut: (keysToLeave) ->
    @data.forEach (object, key, map) =>
      if key not in keysToLeave
        @data.delete(key)
